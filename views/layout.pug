                                                            li(style='display:inline-block')

                                                                  a(href="mailto:exemple.com" title="mail" class="m-1 text-decoration-none toEmail" id="sendEmail")                 

                                    div.row

                                          div.col

                                                ul(class="nav nav-tabs" id="top_menu" style="margin-right:-24px")

                                                      li(class="nav-item" style="")

