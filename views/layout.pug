  body 

      div.container.bg-light

            div.row

                  div.col-md-3.border-end.mb-3

                        nav

                        block nav

                              include views/nav.pug

                  div.col-md-9

                        header(class="border-bottom")

                              div.row.mt-3#section_graph

                                    div.col-md-3

                                          div.form-floating(id="first_div")

                                                select#period_id(class="form-select mb-3" aria-label=".form-select-lg example")

                                                      each $period in $periods

                                                            option(value="$period[0]") =$period[1]+$updateYear

                                                label(for="period_id") Period 

                                    div.col-md-3

                                          div.form-floating(id="second_div")

                                                select#targettype_id(class="form-select mb-3" aria-label=".form-select-lg example")

                                                      

                                                      each $typetarget in $typetargets

                                                            option(value="$typetarget[0]") =$typetarget[1] 

                                                label(for="period_id") Type of Target



                                    div.col-md-3(id="third_div")

                                           div.form-floating

                                                select#target_id(class="form-select mb-3" aria-label=".form-select-lg example")

                                                label(for="period_id") Target



                                    div.col-md-3.float-end

                                          div.row

                                                div.col

                                                      ul(class="list-unstyled")

                                                            li(class="" style='display:inline-block')

                                                                  a(href="disconnect.php" class="m-1 text-decoration-none" data-bs-toggle="tooltip" data-bs-placement="bottom" data-bs-title="Disconnect" id="" style="display: inline-block;border-radius: 5px; box-shadow: 1px 3px 5px;padding: 4px;") <i style="font-size:30px;" class="fa-solid fa-power-off"></i>

                                                            li(class="ms-3" style='display:inline-block')

                                                                  span( data-bs-toggle="modal" data-bs-target="#myModal ")

                                                                        a(href="javascript:void(0);" title="Change password"  data-bs-toggle="tooltip" class="m-1 text-decoration-none" data-bs-placement="bottom" data-bs-title="Change Password" id="" style="display: inline-block;border-radius: 5px; box-shadow: 1px 3px 5px;padding: 4px;") <i style="font-size:30px;"  data-bs-toggle="tooltip" class="fa-solid fa-pen-to-square"></i>

                                                                  div(class="modal" id="myModal") 

                                                                        div(class="modal-dialog modal-lg") 

                                                                              div(class="modal-content")

                                                                                    div(role="alert" id="alertMessageDiv")

                                                                                          p(id="alertMessage" style="font-size:18px;text-align:center;")

                                                                                    div(class="modal-header")

                                                                                          h4(class="modal-title") Change password

                                                                                          button(type="button" class="btn-close" data-bs-dismiss="modal")

                                                                                    div(class="modal-body")

                                                                                          div(class="input-group")

                                                                                                input(class="form-control password block w-full" id="password1" type="password" name="password1"   minlength="6" maxlength="18" placeholder="Current Password" required)

                                                                                                span(class="input-group-text " id="togglePassword1")

                                                                                                      i(data-feather="eye" style="cursor: pointer")

                                                                                          div(class="input-group mt-5")

                                                                                                input(class="form-control password block w-full" id="password2" type="password" name="password2" minlength="6" maxlength="18" placeholder="New Password" required)

                                                                                                span(class="input-group-text " id="togglePassword2")

                                                                                                      i(data-feather="eye" style="cursor: pointer")

                                                                                          div(class="input-group mt-3")

                                                                                                input(class="form-control password block w-full" id="password3" type="password" name="password3" minlength="6" maxlength="18" placeholder="Confirm New Password" required)

                                                                                                span(class="input-group-text " id="togglePassword3")

                                                                                                      i(data-feather="eye" style="cursor: pointer")

                                                                                    div(class="modal-footer")  

                                                                                          button(type="button" class="btn btn-success"  id="updatePassword") Update Password                                

                                          div.row

                                                div.col

                                                      ul(class="list-unstyled")

                                                            li(style='display:inline-block')

