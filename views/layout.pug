                                                      li(class="nav-item" style="")

                                                            a(href="javascript:void(0)" class="nav-link top-menu " id="bytarget"  style="") By target 

   

                        div.row                                  

                              div.col-md-12

                                    ul#displayTypeGraph(style="display:none;list-style:none;float:right;")

